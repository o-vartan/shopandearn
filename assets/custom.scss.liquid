.header__action-item--start_chat {
  display: flex;
  align-items: center;
  padding-left: 18px;
  .start-chat-icon {
    margin-right: 10px;
  }
  .header__action-item-content {
    span {
      display: block;
      &:first-child {
        font-weight: 600;
        font-size: 16px;
        line-height: 24px;
      }
      &:last-child {
        color: var(--header-accent-color);
        font-weight: 500;
        font-size: 14px;
        line-height: 21px;
      }
    }
  }
  svg {
    width: 26px;
    height: 28px;
    path {
      stroke: rgb(255, 255, 255);
    }
  }
}
@media screen and (max-width: 992px) {
  .header__action-item--start_chat {
    padding: 0;
    .start-chat-icon {
      margin: 0 10px;
    }
    svg {
      width: 22px;
      height: 20px;
    }
  }
}
.show_filters {
  padding: 0;
}
.icon-plus,
.icon-minus {
  position: relative;
  &::after {
    display: grid;
    align-items: center;
    content: "\002B";
    margin-left: auto;
    position: absolute;
    top: 0;
    right: 0;
    font-size: 26px;
    line-height: 1;
    font-weight: normal;
    height: 100%;
  }
}
.hide {
  display: none;
}
.show {
  display: block;
}
.absolute-top-right {
  position: absolute;
  top: 0;
  right: 0;
}
.icon-plus {
  &::after {
    content: "\002B";
  }
}
.icon-minus {
  &::after {
    content: "\2212";
  }
}
