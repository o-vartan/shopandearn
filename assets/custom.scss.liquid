.section__permission-main-banner {
  margin: 0;
}

.permission-main-banner__bg {
  position: absolute;
  height: 100%;
  width: 100%;
  object-fit: cover;
}
.permission-main-banner__container {
  padding: 36px 32px;
  display: grid;
  justify-items: center;
  text-align: center;
}
.permission-main-banner__title {
  margin-bottom: 16px;
  font-weight: bold;
  font-size: 36px;
  line-height: 54px;
}
.permission-main-banner__text {
  margin-bottom: 32px;
  font-size: 14px;
  line-height: 21px;
  max-width: 230px;
}

.permission-main-banner__link {
  a {
    background: var(--header-accent-color);
    border-radius: 18px;
    font-weight: bold;
    font-size: 14px;
    line-height: 20px;
    padding: 9px 25px;
  }
}
// end .permission-main-banner
.section__permission-text-with-icons {
  border-bottom: 1px solid var(--border-color);
  margin: 0;
  padding: 42px 0;
}
