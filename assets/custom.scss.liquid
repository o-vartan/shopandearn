  justify-content: center;
  padding-bottom: 0;
  .text-with-icons__icon-wrapper {
  }
  .text-with-icons__content-wrapper {
    display: grid;
    align-items: center;
  }
  .text-with-icons__item {
    margin: 0 20px 0px 20px;
    justify-content: center;
  }
  .text-with-icons__title {
    font-size: 14px;
    line-height: 17px;
  }
